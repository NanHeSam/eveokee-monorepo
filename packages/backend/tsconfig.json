{
  "compilerOptions": {
    "target": "ES2021",
    "lib": ["ES2021"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": false,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "noEmit": true,
    "types": ["node", "vitest/globals", "vite/client"]
  },
  "include": ["./**/*"],
  "exclude": ["node_modules", "_generated"]
}
